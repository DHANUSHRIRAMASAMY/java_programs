//treeMAP
import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    {
	 TreeMap<Integer,String> h=new TreeMap<>();
	     h.put(1,"SIBI");
	     h.put(2,"DHANU");
	     h.put(3,"VANI");
	     h.put(4,"RAMU");
	     h.put(5,"ELAN");
	     h.put(8,"Hello");
	     h.put(9,"World");
	     h.put(6,"null");
		 System.out.println(h);
	    }
	    }
	}

