//enumSet
import java.util.*;
 enum Day
 {
     MONDAY,TUESDAY,WEDNESDAY,THURSDAY,FRIDAY,SATURDAY,SUNDAY;
 }
 class e1
     {
         Day today=Day.MONDAY;
public class Main
{
	public static void main(String[] args) {
	    {
	      e1 week = new e1() ;
		   System.out.println( week.today);
	    }
	}
}
}
